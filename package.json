{
  "name": "ordertracker",
  "version": "1.0.0",
  "description": "Order tracking app for baking business",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.jacobchamney.ordertracker",
    "productName": "Order Tracker",
    "npmRebuild": false,
    "win": {
      "target": "nsis",
      "signingHashAlgorithms": []
    },
    "files": [
      "main.js",
      "renderer.js",
      "index.html",
      "styles.css",
      "node_modules/**/*"
    ]
  },
  "keywords": [],
  "author": "Jacob Chamney",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "better-sqlite3": "^11.8.1"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12"
  }
}
